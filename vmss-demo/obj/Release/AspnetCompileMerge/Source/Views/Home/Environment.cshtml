@model vmss_demo.Models.ServerEnvironmentViewModel
@using System.Collections
@{
    ViewBag.Title = "Environment";
}

<h2>Environment</h2>

<div>
   
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.WorkingSet)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.WorkingSet)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.ServerName)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.ServerName)
        </dd>

    </dl>
</div>
<h2>Environment Variables</h2>
<ul></ul>

        <div>
            <dl class="dl-horizontal">
                <dt>
                    Variable
                </dt>

                <dd>
                    Value
                </dd>

                @{
                    foreach (DictionaryEntry item in Model.EnvironmentVariables)
                    {


                        <dt>
                            @item.Key.ToString()
                        </dt>

                        <dd>
                            @item.Value.ToString()
                        </dd>






                    }
                }
            </dl>      </div>
